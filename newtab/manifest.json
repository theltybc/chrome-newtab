{
	"name": "New Tab Page",
	"version": "0",
	"manifest_version": 2,
	"description": "Display a blank page when opening new tabs.",
	"chrome_url_overrides": {
		"newtab": "blank.html"
	},
	"content_security_policy": "default-src 'self'; script-src 'self'",
	"incognito": "not_allowed",
	"externally_connectable": {
		"ids": []
	},
	"permissions": [
		"storage"
	]
}